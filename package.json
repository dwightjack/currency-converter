    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.10.5",
    "@unocss/core": "0.65.4",
    "@unocss/extractor-svelte": "0.65.4",
    "@unocss/preset-icons": "^0.65.0",
    "@unocss/preset-wind": "^0.65.0",
